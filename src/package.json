{
  "name": "europa",
  "version": "0.1.0",
  "description": "Feature rich thin-client for JupyterLab",
  "homepage": "europa.suyashmahar.com",
  "author": {
    "name": "Suyash Mahar",
    "email": "contact@suyashmahar.com"
  },
  "main": "main.js",
  "repository": "https://github.com/suyashmahar/europa",
  "keywords": [
    "JupyterLab",
    "Jupyter"
  ],
  "license": "AGPLv3",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^3.0.4",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "command-exists": "^1.2.9",
    "electron-localshortcut": "^3.2.1",
    "electron-reload": "^1.2.5",
    "electron-store": "^2.0.0",
    "spectre.css": "^0.5.3"
  },
  "build": {
    "appId": "com.suyashmahar.europa",
    "mac": {
      "category": "Utilities"
    },
    "linux": {
      "category": "Utility",
      "target": ["AppImage", "snap", "deb", "rpm", "freebsd", "pacman", "zip", "tar.xz", "tar.gz"]
    },
    "win": {
      "target": ["nsis"]
    }
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "start": "electron .",
    "dist": "electron-builder"
  }
